    "class-validator": "0.14.1",
    "dotenv": "catalog:",
    "ethers": "catalog:",
    "express": "catalog:",
    "filtrex": "3.1.0",
    "pg": "8.13.1",
    "prom-client": "catalog:",
    "typeorm": "0.3.28",
    "typeorm-naming-strategies": "4.1.0",
    "winston": "catalog:"
