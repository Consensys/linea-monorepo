{
  "name": "bridge-ui",
  "version": "2.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && pnpm run lint:test",
    "lint:test": "npx eslint 'test/**/*.{js,ts}'",
    "lint:fix": "pnpm run lint:ts:fix && pnpm run lint:test:ts:fix",
    "lint:ts:fix": "next lint --fix",
    "lint:test:ts:fix": "npx eslint --fix 'test/**/*.{js,ts}'",
    "clean": "rimraf node_modules .next .next-env.d.ts",
    "install:playwright": "playwright install --with-deps",
    "build:cache": "synpress",
    "build:cache:force": "synpress --force",
    "build:cache:headless": "synpress --headless",
    "test:unit": "HEADLESS=true playwright test src",
    "test:e2e:headful": "playwright test test/e2e",
    "test:e2e:headless": "HEADLESS=true playwright test test/e2e",
    "test:e2e:headless:ui": "HEADLESS=true playwright test test/e2e --ui"
  },
  "dependencies": {
    "@consensys/linea-sdk": "workspace:*",
    "@dynamic-labs/ethereum": "4.16.0",
    "@dynamic-labs/sdk-react-core": "4.16.0",
    "@dynamic-labs/solana": "4.16.0",
    "@dynamic-labs/wagmi-connector": "4.16.0",
    "@headlessui/react": "2.1.9",
    "@lifi/widget": "3.20.4",
    "@next/bundle-analyzer": "15.3.1",
    "@solana/wallet-adapter-base": "0.9.26",
    "@solana/wallet-adapter-react": "0.15.38",
    "@solana/web3.js": "1.98.2",
    "@tanstack/react-query": "5.75.4",
    "@wagmi/connectors": "5.8.1",
    "@wagmi/core": "2.16.7",
    "@zod/mini": "4.0.0-beta.0",
    "auto-zustand-selectors-hook": "3.0.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "loglevel": "1.9.2",
    "motion": "^12.10.0",
    "next": "15.3.1",
    "next-seo": "6.6.0",
    "pino-pretty": "13.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "5.5.0",
    "sharp": "0.33.5",
    "viem": "2.25.0",
    "wagmi": "2.14.16",
    "zustand": "4.5.4"
  },
  "devDependencies": {
    "@playwright/test": "1.51.1",
    "@svgr/webpack": "8.1.0",
    "@synthetixio/synpress": "4.0.10",
    "@types/fs-extra": "11.0.4",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "autoprefixer": "10.4.21",
    "dotenv": "16.4.7",
    "eslint-config-next": "15.3.1",
    "nock": "14.0.1",
    "postcss": "8.5.3",
    "sass": "1.87.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.3",
      "@types/react-dom": "19.1.3"
    }
  }
}
