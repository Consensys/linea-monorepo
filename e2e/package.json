{
  "name": "e2e",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test:e2e:local": "TEST_ENV=local jest --testPathIgnorePatterns=linea-besu-fleet.spec.ts --testPathIgnorePatterns=liveness.spec.ts && LOG_LEVEL=debug pnpm run test:e2e:liveness:local",
    "test:e2e:fleet:local": "TEST_ENV=local jest ./src/linea-besu-fleet.spec.ts",
    "test:e2e:liveness:local": "TEST_ENV=local jest ./src/liveness.spec.ts",
    "test:e2e:sendbundle:local": "TEST_ENV=local jest ./src/send-bundle.spec.ts",
    "test:e2e:dev": "TEST_ENV=dev jest --config ./jest.testnet.config.ts --bail --runInBand --testPathIgnorePatterns=restart.spec.ts --testPathIgnorePatterns=linea-besu-fleet.spec.ts --testPathIgnorePatterns=liveness.spec.ts",
    "test:e2e:sepolia": "TEST_ENV=sepolia jest --config ./jest.testnet.config.ts --bail --runInBand --testPathIgnorePatterns=restart.spec.ts --testPathIgnorePatterns=linea-besu-fleet.spec.ts --testPathIgnorePatterns=liveness.spec.ts",
    "postinstall": "typechain --target ethers-v6 --out-dir ./src/typechain '../contracts/local-deployments-artifacts/**/*.json'",
    "clean": "rimraf node_modules src/typechain"
  },
  "author": "",
  "license": "(MIT OR Apache-2.0)",
  "devDependencies": {
    "@consensys/eslint-config": "workspace:*",
    "@jest/globals": "catalog:",
    "@openzeppelin/upgrades-core": "1.42.1",
    "@typechain/ethers-v6": "catalog:",
    "@types/jest": "catalog:",
    "async-mutex": "^0.5.0",
    "child_process": "1.0.2",
    "dotenv": "catalog:",
    "ethers": "catalog:",
    "jest": "catalog:",
    "ts-jest": "catalog:",
    "typechain": "catalog:",
    "winston": "catalog:"
  }
}
