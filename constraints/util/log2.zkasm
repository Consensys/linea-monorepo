;; Compute log2 of word
fn log2(arg u256) -> (res u8) {
  var hi, lo u128
  var tmp u7
  ;; split argument
  hi,lo = arg
  ;; check hi or lo
  if hi == 0 goto low
  ;; hi mode
  tmp = log2_u128(hi)
  res = tmp + 128
  return
low:
  tmp = log2_u128(lo)
  res = tmp
  return
}

