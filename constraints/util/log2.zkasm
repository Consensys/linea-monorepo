fn log2_u2(arg u2) -> (res u1) {
  var hi, lo u1
  ;; split argument
  hi,lo = arg
  ;; check hi or lo
  if hi == 0 goto low
  ;; hi
  res = 1
  return
low:
  res = 0
  return
}
