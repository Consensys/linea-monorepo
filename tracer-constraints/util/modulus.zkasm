;; compute a mod b where a is a u512 and a u256
;; a mod 0 = 0
fn modulus_u512_u256(A u512, M u256) -> (RES u256) {
  var quot u512
  var sum u768
  var witsum u769
  var witness u256
  ;; handle division by 0
  if M == 0 goto exit_0
  quot, RES, witness, sum, witsum = A / M
  return
exit_0:
  RES = 0
  return
}
